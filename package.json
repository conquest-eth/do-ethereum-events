{
  "name": "@helios138/ethereum-events-worker",
  "version": "0.0.1",
  "description": "Event Engine For Ethereum",
  "module": "./dist/index.mjs",
  "scripts": {
    "ts-node": "ts-node",
    "prepare": "node .setup.cjs",
    "build:cjs": "node build.cjs",
    "build": "npm-run-all \"generate -- {1}\" \"build:cjs\" --",
    "format": "prettier --write '**/*.{js,css,json,md}'",
    "generate": "node generate.cjs",
    "deploy:wrangler": "wrangler publish",
    "deploy": "npm-run-all \"generate -- {1}\" \"deploy:wrangler\" --",
    "dev": "npm-run-all \"generate -- {1}\" \"dev:flare\" --",
    "dev:flare": "miniflare --watch --debug",
    "manual-test": "esbuild test/manual.ts --outfile=dist/manual.mjs && node dist/manual.mjs"
  },
  "author": "wighawag <wighawag@gmail.com>",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.3.0",
    "@cloudflare/wrangler": "^1.19.7",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.10",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "isomorphic-fetch": "^3.0.0",
    "miniflare": "^2.0.0-rc.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "ethers": "^5.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conquest-eth/ethereum-events-worker"
  },
  "volta": {
    "node": "16.13.1"
  }
}
