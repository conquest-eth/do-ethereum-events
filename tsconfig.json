{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext"],
    "types": ["@cloudflare/workers-types"],
    "moduleResolution": "node",
    "strict": true,
    // "noEmit": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
    "composite": true
  },
  "include": ["src/**/*", "env.d.ts"],
  "references": [
    {
      "path": "node_modules/do-json-db"
    }
  ]
}
