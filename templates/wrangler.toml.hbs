name = "ethereum-events-worker-{{environment}}"
type = "javascript"
account_id = ""
workers_dev = {{devMode}}
route = ""
zone_id = ""
compatibility_date = "2021-09-21"
usage_model= "bundled"

[vars]
ENVIRONMENT = "{{environment}}"
ETHEREUM_NODE = "{{ETHEREUM_NODE}}"
DATA_DOG_API_KEY = "{{DATA_DOG_API_KEY}}"

[build]
command = "npm run build:cjs"
[build.upload]
format = "modules"
main = "./index.mjs"

[durable_objects]
bindings = [{name = "EVENT_LIST", class_name = "VoidEventList"}]

[triggers]
crons = ["* * * * *"]

