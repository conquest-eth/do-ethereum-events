{
  "compilerOptions": {
    "emitDeclarationOnly": true,
    "declaration": true,
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext"],
    "outDir": "dist",
    "types": ["@cloudflare/workers-types"],
    "moduleResolution": "node",
    "strict": true,
    // "noEmit": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*", "env.d.ts"]
}
